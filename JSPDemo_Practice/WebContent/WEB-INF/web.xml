<?xml version="1.0" encoding="UTF-8"?>

<web-app>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>vs</servlet-name>
		<jsp-file>/validate.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>vs</servlet-name>
		<url-pattern>/valid</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
</web-app>